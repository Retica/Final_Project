-- Table for 1991

SELECT hpi_type, hpi_flavor, frequency, level, place_name, 
place_id, yr, period, index_nsa, index_sa
INTO ninetyone_table
FROM hpitype
WHERE (place_id BETWEEN '1991' AND '1991');

SELECT * FROM ninetyone_table;


-- Table for 1992
SELECT hpi_type, hpi_flavor, frequency, level, place_name, 
place_id, yr, period, index_nsa, index_sa
INTO ninetytwo_table
FROM hpitype
WHERE (place_id BETWEEN '1992' AND '1992');

SELECT * FROM ninetytwo_table;

