-- HOUSEHOLD DEBT


CREATE TABLE householdDebt (
	year INT NOT NULL,
	qtr INT NOT NULL,
	state_fips INT NOT NULL,
	low VARCHAR,
	high VARCHAR
);

DROP TABLE householdDebt;

SELECT * FROM householdDebt;








































