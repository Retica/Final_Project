-- use this as template. Adjust to what will be needed for the csv. then you create
--CREATE table and should see in schemas and then should be able to add csv from there. --

CREATE TABLE HPItype (
	hpi_type VARCHAR (25) NOT NULL,
	hpi_flavor VARCHAR NOT NULL,
	frequency VARCHAR NOT NULL,
	level VARCHAR NOT NULL,
	place_name VARCHAR (40) NOT NULL,
	place_id VARCHAR (10) NOT NULL,
	yr VARCHAR NOT NULL,
	period INT NOT NULL,
	index_nsa FLOAT NOT NULL,
	index_sa FLOAT
);

DROP TABLE HPItype;

SELECT * FROM HPItype;

