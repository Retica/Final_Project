
CREATE TABLE foreclosure (
DATE VARCHAR NOT NULL,
DRSFRMACBS FLOAT NOT NULL
);

SELECT * FROM foreclosure;

-- Table for 1991

SELECT date, drsfrmacbs
INTO ninetyone_table_foreclosure
FROM foreclosure
WHERE (date BETWEEN '1991-01-01' AND '1991-10-01');

SELECT * FROM ninetyone_table_foreclosure;
DROP TABLE ninetyone_table_foreclosure;

-- Table for 1992
SELECT date, drsfrmacbs
INTO ninetytwo_table_foreclosure
FROM foreclosure
WHERE (date BETWEEN '1992-01-01' AND '199210-01');

